import ddf.minim.*;
import ddf.minim.signals.*;
import ddf.minim.analysis.*;
import ddf.minim.effects.*;
Minim minim;
AudioPlayer song;
void setup(){
  size(100, 100);
  minim = new Minim(this);
  // this loads mysong.wav from the data folder
  song = minim.loadFile("AlphabetSong.mp3");
  song.play();
}
void draw() { 
  background(0);
}
void stop() {
  song.close();
  minim.stop();
  super.stop();
}
