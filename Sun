color c = color(254, 255, 23);
color c2 = color(255,205,0);

void setup() {
  size(800, 800);
}
void draw() {
  background(255);
  stroke(3);
 
  fill(c);
  
  ellipse(0, 0, 300, 300);
 
  fill(c2);
  
  triangle(183,28,330,66,174,64);
  
  triangle(168,144,128,136,210,214);
  
  triangle(53,182,20,190,36,280);
  
}
